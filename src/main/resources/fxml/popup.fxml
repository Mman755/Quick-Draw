<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="214.0" prefWidth="279.0" stylesheets="@../css/app.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.PopupController">
   <children>
      <Label layoutX="6.0" layoutY="1.0" prefHeight="135.0" prefWidth="283.0" styleClass="message" text="Nice work! Are you ready for a bigger challenge?" textAlignment="CENTER" wrapText="true">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Label>
      <Button fx:id="yesBtn" layoutX="28.0" layoutY="147.0" mnemonicParsing="false" onAction="#onGameSettingsChange" text="Yes" />
      <Button fx:id="noBtn" layoutX="194.0" layoutY="147.0" mnemonicParsing="false" onAction="#onGameSettingsChange" text="No" />
   </children>
</AnchorPane>
